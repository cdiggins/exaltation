{{#rss}}<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
<channel>
 <title>{{title}}</title>
 <description>{{description}}</description>
 <link>{{siteUrl}}</link>
 <lastBuildDate>{{built}}</lastBuildDate>
 <webMaster>{{author}}</webMaster>
 <managingEditor>{{author}}</managingEditor>
 <copyright>Copyright 2017, {{author}}</copyright>
 <generator>Exaltation</generator>
 <language>en-us</language>
 
 {{#articles}}
 <item>
  <title>{{title}}</title>
  <description>{{description}}</description>
  <category>{{category}}</category>
  <link>{{pageUrl}}</link>
  <source>http://cdiggins.github.io/rss.xml</source>
  <pubDate>{{rssDate}}</pubDate>
 </item>
 {{/articles}}

</channel>
</rss>{{/rss}}